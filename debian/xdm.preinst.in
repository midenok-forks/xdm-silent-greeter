set -e

THIS_PACKAGE=xdm
THIS_SCRIPT=preinst

#INCLUDE_SHELL_LIB#

if [ "$1" = "install" ]; then
  # Create a flag file that tells the postinst script this is an install, not
  # an upgrade.
  : >/var/run/xdm.install
fi

#DEBHELPER#

exit 0

